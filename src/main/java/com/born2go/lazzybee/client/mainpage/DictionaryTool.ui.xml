<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .DictionaryTool_Obj1, .DictionaryTool_Obj5, .DictionaryTool_Obj7, .DictionaryTool_Obj8, .DictionaryTool_Obj9;
	.DictionaryTool_Obj1 {
		float: left;
		width: 100%;
		background: #009688;
		text-align: center;
		position: relative;
	}
	
	.DictionaryTool_Obj2 {
		float: left;
		padding: 30px;
		width: 100%;
	}
	
	.DictionaryTool_Obj3 {
		overflow: hidden;
		display: inline-flex;
		display: -webkit-inline-flex;
		display: -ms-inline-flexbox;
		width: 100%;
	}
	
	.DictionaryTool_Obj4 {
		color: white !important;
		cursor: pointer;
		margin-top: 40px;
	}
	
	.DictionaryTool_Obj4:hover {
		color: silver !important;
		text-decoration: none;
	}
	
	.DictionaryTool_Obj5 {
		color: rgb(234, 253, 116) !important;
	}
	
	.DictionaryTool_Obj6 {
		color: white;
		line-height: 1.5;
		padding: 5px;
   	 	width: 100%;
    	box-sizing: border-box;
	}
	
	.DictionaryTool_Obj7 {
		color: #333 !important;
   	 	font-weight: bold;
   	 	font-size: 13px;
    	margin-top: 10px;
    	display: block;
    	/* height: 35px; */
    	position: absolute;
    	top: 165px;
    	padding-bottom: 10px;
	}
	
	.DictionaryTool_Obj7:hover {
		color: #0e74af !important;
		cursor: pointer;
		text-decoration: none !important;
	}
	
	.DictionaryTool_Obj8 {
		cursor: pointer;
		width: 180px; 
		height: 150px;
		padding-bottom: 57px;
	}
	
	.DictionaryTool_Obj9 {
		width: 183px; 
		/* overflow: hidden;  */
		padding-top: 15px; 
		float: left; 
		margin-right: 15px; 
		margin-left: 2px; 
		margin-bottom: 10px;
		position: relative;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.DictionaryTool_Obj3}">
		<g:HTMLPanel ui:field="tabPanel" styleName="{style.DictionaryTool_Obj1}">
			<br/> <br/>
			
			<g:Anchor ui:field="defaultSite" styleName="{style.DictionaryTool_Obj4}">Tra cứu từ vựng</g:Anchor>
			<br/> <br/> <br/>
			<g:Anchor ui:field="testSite" styleName="{style.DictionaryTool_Obj4}">Kiểm tra vốn từ</g:Anchor>
			<br/> <br/> <br/>
			<g:Anchor ui:field="dictionarySite" visible="false" styleName="{style.DictionaryTool_Obj4}">Từ điển</g:Anchor>
			<br/> <br/> <br/>
			<g:Anchor ui:field="previewSite" visible="false" styleName="{style.DictionaryTool_Obj4}">Bảng từ mới</g:Anchor>
			
			<g:Label ui:field="trademarkLb" styleName="{style.DictionaryTool_Obj6}">Born2Go © 2016</g:Label>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 