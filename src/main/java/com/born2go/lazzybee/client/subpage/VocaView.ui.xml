<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .VocaView_Obj5;
	.VocaView_Obj1 {
		margin-top: 30px;
	}
	
	.VocaView_Obj2 {
		float: left;
	}
	
	.VocaView_Obj3 {
		float: right;
	}
	
	.VocaView_Obj4 {
		color: #0e74af;
    	font-weight: bold;
    	font-size: 18px;
	}
	
	.VocaView_Obj5 {
		padding: 10px;
		padding-left: 5px;
		background: #E0E0E0;
	}
	
	.VocaView_Obj6 {
		margin-left: 15px;
		margin-top: 2px;
	}
	
	.VocaView_Obj6:hover {
		color: silver;
		cursor: pointer;
	}
	
	.VocaView_Obj7 {
		margin-bottom: 20px;
    	padding: 10px;
    	background-color: lemonchiffon;
    	line-height: 1.5;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.VocaView_Obj1}">
		<g:HTMLPanel styleName="{style.VocaView_Obj7}" ui:field="htmlVocaNote" visible="false"></g:HTMLPanel>
		<g:HTMLPanel>
			<g:Label ui:field="lbVocaQ" styleName="{style.VocaView_Obj2} {style.VocaView_Obj4}"></g:Label>
			<g:Anchor ui:field="btnEdit" styleName="{style.VocaView_Obj2} {style.VocaView_Obj6}" title="Edit"><i class="fa fa-pencil-square-o fa-lg"></i></g:Anchor>
			<g:Anchor ui:field="btnVocaPronounce" styleName="{style.VocaView_Obj3}"><i class="fa fa-volume-up fa-lg"></i></g:Anchor>
		</g:HTMLPanel>
		
		<br/> <br/>
		
		<g:HTMLPanel>
			<g:Label ui:field="lbVocaLevel" styleName="{style.VocaView_Obj2}"></g:Label>
			<g:Label ui:field="lbVocaPronoun" styleName="{style.VocaView_Obj3}"></g:Label>
		</g:HTMLPanel>
		 
		<br/> <br/>
		
		<g:HTMLPanel ui:field="htmlDefiTable"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 