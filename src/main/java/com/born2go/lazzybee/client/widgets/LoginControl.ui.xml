<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .LoginControl_clean, .LoginControl_Obj2, .LoginControl_Obj3;
	
	.LoginControl_clean{}
	
	.LoginControl_Obj1 {
		width: 300px;
		height: 60px;
	}
	
	.LoginControl_Obj1:hover {
		cursor: pointer;
	}
	
	.LoginControl_Obj2 {
		width: 200px;
		height: 50px;
		background: #009688;
		box-shadow: 0 4px 2px -2px gray;
	}
	
	.LoginControl_Obj3 {
		color: white;
		width: 200px;
		text-align: center;
		padding-top: 10px;
		padding-bottom: 10px;
		margin-top: 5px;
		margin-left: -1px;
	}
	
	.LoginControl_Obj3:hover {
		background: silver;
		cursor: pointer;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:Image url="../resources/google_login_button.png" ui:field="googleLogin" styleName="{style.LoginControl_Obj1}"></g:Image>
		<g:Image url="../resources/facebook_login_button.png" ui:field="facebookLogin" styleName="{style.LoginControl_Obj1}"></g:Image>
	</g:HTMLPanel>
</ui:UiBinder> 