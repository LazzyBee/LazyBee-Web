<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .MVocaView_Obj5;
	.MVocaView_Obj1 {
		margin-top: 10px;
	}
	
	.MVocaView_Obj2 {
		float: left;
	}
	
	.MVocaView_Obj3 {
		float: right;
	}
	
	.MVocaView_Obj4 {
		color: #009688;
    	font-weight: bold;
    	font-size: 18px;
	}
	
	.MVocaView_Obj5 {
		padding: 10px;
		padding-left: 5px;
		background: #E0E0E0;
	}
	
	.MVocaView_Obj6 {
		margin-left: 15px;
		margin-top: 2px;
	}
	
	.MVocaView_Obj6:hover {
		color: silver;
		cursor: pointer;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.MVocaView_Obj1}">
		<g:HTMLPanel>
			<g:Label ui:field="lbVocaQ" styleName="{style.MVocaView_Obj2} {style.MVocaView_Obj4}"></g:Label>
			<g:Anchor ui:field="btnVocaPronounce" styleName="{style.MVocaView_Obj3}"><i class="fa fa-volume-up fa-lg"></i></g:Anchor>
		</g:HTMLPanel>
		
		<br/> <br/>
		
		<g:HTMLPanel>
			<g:Label ui:field="lbVocaLevel" styleName="{style.MVocaView_Obj2}"></g:Label>
			<g:Label ui:field="lbVocaPronoun" styleName="{style.MVocaView_Obj3}"></g:Label>
		</g:HTMLPanel>
		 
		<br/> <br/>
		
		<g:HTMLPanel ui:field="htmlDefiTable"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 